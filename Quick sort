#include<stdio.h>
void qs(int[],int,int);
 int split(int[],int,int);
 int main(){
    int a[10],i,n,lb,ub;
    printf("enter array size:");
    scanf("%d",&n);
    printf("enter the elemenmt :\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    lb=0;
    ub=n-1;
    qs(a,lb,ub);
    for ( i = 0; i <n; i++)
    {
      printf("%d\n",a[i]);

    }
    return 0;
    
 }
 void qs(int a[],int lb,int ub){
    int q; 
    if(lb<ub){
        q=split(a,lb,ub);
        qs(a,lb,q-1);
        qs(a,q+1,ub);

    }

 }
 int split(int a[],int lb,int ub){
    int p,q,temp;
    p=lb+1;
    q=ub;
    while(q>=p){
        while(q[a]>=a[lb]&&q>lb){
            q--;

        }
        while(a[p]<a[lb]&& p<=ub){
            p++;
        }
        if(q>p){
            temp=a[q];
            a[q]=a[p];
            a[p]=temp;
        }
        temp=a[lb];
        a[lb]=a[q];
        a[q]=temp;
        return q;

    }

 }
